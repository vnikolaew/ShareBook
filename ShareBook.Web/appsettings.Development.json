{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "AzureBlobStorageSettings": {
        "AccountName": "<AzureBlob_Account_Name>",
        "AccountKey": "<AzureBlob_Account_Key>",
        "EndpointSuffix": "<Endpoint_Suffix>",
        "ContainerName": "images"
    },
    "JwtSettings": {
        "AppSecret": "Some super cool secret",
        "ValidIssuer": "localhost",
        "ValidAudience": "localhost",
        "ValidateLifetime": true,
        "ValidateActor": true,
        "RelativeExpirationInHours": 1
    },
    "GraphDatabaseSettings": {
        "Username": "<your_username>",
        "Password": "<your_password>",
        "BoltEndpoint": "<database_endpoint>",
        "DefaultDatabase": "neo4j",
        "UseDataSeeding": true
    }
}
